
local COUNTER = 1

local function myOutput()

    local player = getPlayer()
    --[[local items = player:getInventory():getItems()
    print("Iteration: ", COUNTER)
    for i=1,items:size() do 
          print("Item: ", items:get(i-1):getFullType())
    end]]
    
    local penis = player:getWornItem("RasMalePrivatePart")
    if penis ~= nil then
         print(penis:getFullType())
    else
         print("None")
    end

    COUNTER = COUNTER + 1

end


Events.EveryOneMinute.Add(myOutput)




local function onTick(tick)
   print("TICK")
end

--Events.OnTick.Add(onTick)



local function onUpdate(player)
   print("UPDATE")
end

